-- update & draw

function _update()
 if not env.game_won then
  -- input
	 player_input()
	 -- player movement / collision
	 move_and_collide_player()
	 -- enemy movement / collision
	 for i = 1, #level_1.ground_enemies do
		 ground_enemy = level_1.ground_enemies[i]
		 if (not ground_enemy.dead) then
			 move_and_collide_enemy(ground_enemy)
		 end
	 end
	 for i = 1, #level_1.flying_enemies do
	  local e = level_1.flying_enemies[i]
	  move_and_collide_flying_enemy(e)
	 end
	 local b = level_1.boss
	 move_and_collide_boss(b)
	end
end

function _draw()
 if not env.game_won then 
  -- update camera
  camera(max(0, plr.x-64), 0)
  -- draw the map
	 map()
	 -- draw the player
	 spr(plr.sprite, plr.x, plr.y, 1, 1, plr.flipped)
	 -- draw enemies
	 -- for now we are only using
	 -- level 1
	 for i = 1, #level_1.ground_enemies do
	 	ground_enemy = level_1.ground_enemies[i]
		 if (not ground_enemy.dead) then
		  spr(ground_enemy.sprite,ground_enemy.x,ground_enemy.y,1,1,ground_enemy.flipped)
	 	end
	 end
	
	 -- draw flying enemies
	 for i = 1, #level_1.flying_enemies do
	  local e = level_1.flying_enemies[i]
	  if not e.dead then
	   if e.flipped then
	    spr(e.sprite_2, e.x, e.y, 1, 1, e.flipped)
	    spr(e.sprite_1, e.x+8, e.y, 1, 1, e.flipped)
	   else
	    spr(e.sprite_1, e.x, e.y, 1, 1, e.flipped)
	    spr(e.sprite_2, e.x+8, e.y, 1, 1, e.flipped)
	   end
	  end
	 end
	
	 -- draw boss
	 local b = level_1.boss
	 if not b.flipped then
	  spr(b.sprite_1, b.x, b.y, 1, 1, b.flipped)
	  spr(b.sprite_2, b.x+8, b.y, 1, 1, b.flipped)
	  spr(b.sprite_3, b.x,b.y+8, 1, 1, b.flipped)
	  spr(b.sprite_4, b.x+8, b.y+8, 1, 1, b.flipped)
	 else 
	  spr(b.sprite_2, b.x, b.y, 1, 1, b.flipped)
	  spr(b.sprite_1, b.x+8, b.y, 1, 1, b.flipped)
	  spr(b.sprite_4, b.x,b.y+8, 1, 1, b.flipped)
	  spr(b.sprite_3, b.x+8, b.y+8, 1, 1, b.flipped)
	 end
 else
 	camera(0,0)
  rectfill(0, 0, 128, 128, 0)
  print("you win", 52, 64, 3)
  if not env.game_win_sound_played then
   sfx(3)
   env.game_win_sound_played=true
  end
 end
end


