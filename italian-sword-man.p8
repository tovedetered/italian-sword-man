-- game state
function reset_game()
	plr.x = 8
	plr.y = 104
	for i = 1, #level_1.ground_enemies do
	 local e = level_1.ground_enemies[i]
	 e.dead = false
	end
	for i = 1, #level_1.flying_enemies do
	 local e = level_1.flying_enemies[i]
	 e.dead = false
	end
	level_1.boss.hp=3
	sfx(2)
end


